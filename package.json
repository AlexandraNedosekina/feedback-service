	},
	"dependencies": {
		"@emotion/react": "^11.10.4",
		"@emotion/server": "^11.10.0",
		"@mantine/core": "^5.3.3",
		"@mantine/hooks": "^5.3.3",
		"@mantine/next": "^5.3.3",
		"next": "12.3.0",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@types/node": "18.7.18",
		"@types/react": "18.0.20",
		"@types/react-dom": "18.0.6",
		"eslint": "8.23.1",
		"eslint-config-next": "12.3.0",
		"eslint-config-prettier": "^8.5.0",
		"husky": "^8.0.1",
		"jest": "^29.0.3",
		"jest-environment-jsdom": "^29.0.3",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"typescript": "4.8.3"
